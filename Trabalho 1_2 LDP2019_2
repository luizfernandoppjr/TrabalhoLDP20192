import numpy as np

class Pessoa:
    def __init__(self, idade, peso):
        self.idade = idade
        self.peso = peso


class Populacao:
    def __init__(self, tamanho):
        self.tamanho = tamanho
        med = []
        cv = []
        desv = cv*med
        self.individuos = [Pessoa(np.random.normal(med, desv)), randrange(50,150)) for i in range(tamanho)]
    def amostra(self, n):
        pass


pop = Populacao(100)