import numpy as np

class Pessoa:
    def __init__(self, rendimento_mensal_domiciliar, rede_ensino, modalidade, sexo, cor, regiao, idade):
        self.rendimento_mensal_domiciliar = rendimento_mensal_domiciliar
        self.rede_ensino = rede_ensino
        self.modalidade = modalidade
        self.sexo = sexo
        self.cor = cor
        self.regiao = regiao
        self.idade = idade


class Populacao:
    def __init__(self, tamanho):
        self.tamanho = tamanho
        med = []
        cv = []
        desv = cv*med
        self.individuos = [Pessoa(np.random.normal(med, desv)), randrange(50,150)) for i in range(tamanho)]
    def amostra(self, n):
        pass


pop = Populacao(100)